pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
function _init()
	player={}
	player.x=24
	player.y=24
end

function _update()
	cls()
	
	if(btn(0)) then
		player.x-=1
		anim(player,6,3,10,true)
	elseif(btn(1)) then
		player.x+=1
		anim(player,6,3,10)
	elseif(btn(2)) then
		player.y-=1
		anim(player,3,3,10)
	elseif(btn(3)) then
		player.y+=1
		anim(player,0,3,10)
	else spr(0,player.x,player.y)
	end
end

--object, start frame,
--num frames, speed, flip
function anim(o,sf,nf,sp,fl)
	if(not o.a_ct) o.a_ct=0
	if(not o.a_st)	o.a_st=0

	o.a_ct+=1

	if(o.a_ct%(30/sp)==0) then
	 o.a_st+=1
	 if(o.a_st==nf) o.a_st=0
	end

	o.a_fr=sf+o.a_st
	spr(o.a_fr,o.x,o.y,1,1,fl)

	print("a_ct"..o.a_ct,2,2,3)
	print("a_st"..o.a_st,2,8,3)
end
